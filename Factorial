import java.util.Scanner;
public class Main{
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter your number: ");
        int n = sc.nextInt();

        factorial(n);
    }
    public static void factorial(int n){
        float p = 1;
        for(int i = n; i >= 1; i --){
            p = p * i;
        }
        System.out.println(n + " Factorial is: " + p);
    }
}